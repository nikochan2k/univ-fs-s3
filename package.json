{
  "name": "univ-fs-s3",
  "version": "0.7.2",
  "description": "The universal Filesystem API for S3",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "fs",
    "filesystem",
    "storage",
    "isomorphic",
    "universal",
    "s3"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "npm-version": "npm version patch",
    "npm-publish": "npm publish",
    "sanitize": "rimraf ./lib/**/secret*.json",
    "release": "npm-run-all clean sanitize compile npm-version npm-publish",
    "test": "jest --runInBand",
    "webpack": "webpack",
    "web-compile": "npm-run-all compile webpack",
    "web-build": "npm-run-all clean web-clean web-compile",
    "webpack-test": "webpack --config webpack.config.test.js",
    "web-compile-test": "npm-run-all compile webpack-test",
    "karma": "karma start",
    "web-test": "npm-run-all web-compile-test karma"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/src/__tests__/**/*.spec.+(ts|tsx|js)"
    ],
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "jasmine": "^4.1.0",
    "jest": "^28.0.3",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.82.0",
    "@aws-sdk/lib-storage": "^3.82.0",
    "@aws-sdk/s3-request-presigner": "^3.82.0",
    "tslib": "^2.4.0",
    "univ-fs": "^0.7.19"
  },
  "author": "Yoshihiro Seki",
  "license": "MIT"
}
